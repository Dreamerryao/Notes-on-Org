{
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css"
    },
    "plugins": [
        "emphasize",
        "-highlight",
        "tbfed-pagefooter",
        "advanced-emoji",
        "todo",
        "flexible-alerts", 
        "prism", 
        "prism-themes",
        "pageview-count"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright": "Copyright",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-solarizedlight.css"
            ],
            "ignore":[
                "asm"
            ]
          },
          "flexible-alerts": {
            "style": "flat"
          }
    }
}